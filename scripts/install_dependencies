#!/bin/bash
yum install -y httpd
deploymentid=$(aws deploy get-deployment-group --application-name nht --deployment-group-name nht-ec2 --region ap-northeast-1 --query deploymentGroupInfo.deploymentGroupId --output text)

#echo $deploymentid
sudo echo /var/www/html/index.html >> /opt/codedeploy-agent/deployment-root/deployment-instructions/$deploymentid-cleanup
#yum install -y git
